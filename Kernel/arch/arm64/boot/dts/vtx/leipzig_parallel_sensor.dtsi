&soc {
AR0234_0:AR0234@0 {
		compatible = "vatics,ar0234";
		data_io_num = <0>; /*DVP0=0, DVP1=1, DVP2=2, DVP3=3, MIPI0=4, MIPI1=5, LVDS0=6, LVDS1=7*/
		ref_clock_from_soc = <0>;
		reset_gpio_num = <0xFFF>;
		standby_gpio_num = <0xFFF>;
		sensor_i2c_bus_num = <1>;
		sensor_i2c_addr = <0x30>;
		frame_sync_mode = <0>;
		frame_sync_signal_num = <0xFFF>;
		clocks = <&clks CLK_VIC_O_REF>;
		clock-names = "vic_o_ref_clk";
	};
