NullSensor_0:NullSensor@0 {
		compatible = "vatics,nullsensor";
		data_io_num = <0>; /*Input port used by sensor, DVP0=0, DVP1=1, DVP2=2, DVP3=3, MIPI0=4, MIPI1=5, LVDS0=6, LVDS1=7*/
		ref_clock_from_soc = <0>; /*Enable SOC PLL clock as sensor input clock, disable=0, enable=1*/
		reset_gpio_num = <0xFFF>; /*GPIO number used for sensor HW reset. (0xFFF : Ignore)*/
		standby_gpio_num = <0xFFF>; /*GPIO number used for sensor Power down. (0xFFF : Ignore)*/
		clocks = <&clks CLK_VIC_O_REF>;
		clock-names = "vic_o_ref_clk";
	};
