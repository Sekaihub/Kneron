&i2c0 {
	status = "okay";
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&i2c0_pins>;
	/* HDMI Transmitter device: AD ADV7513 */
	adv7513@39 {
		compatible = "adi,adv7513";
		reg = <0x39>;
	};
	/* Audio Codec device: AD SSM2603 */
	ssm2603@1a {
		status = "okay";
		compatible = "wlf,ssm2603";
		reg = <0x1a>;
	};
};

